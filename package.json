{
  "name": "cloud189-sdk",
  "version": "1.0.5",
  "description": "基于node.js的第三方天翼云盘SDK",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!__tests__"
  ],
  "scripts": {
    "start": "node ./test/app.js",
    "build": "tsc",
    "api-extractor": "api-extractor run",
    "api-documenter": "api-documenter markdown -i temp -o docs/api",
    "doc:preare": "npm run api-extractor && npm run api-documenter",
    "doc:dev": "npm run doc:preare && vuepress dev docs",
    "test": "mocha",
    "coverage": "nyc npm run test"
  },
  "author": "wes lin",
  "repository": {
    "type": "git",
    "url": "https://github.com/wes-lin/cloud189-sdk.git"
  },
  "keywords": [
    "天翼网盘sdk"
  ],
  "license": "MIT",
  "dependencies": {
    "got": "11.8.2",
    "node-jsencrypt": "^1.0.0",
    "tough-cookie": "^4.1.4"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.26.9",
    "@microsoft/api-extractor": "^7.50.0",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.13",
    "@typescript-eslint/parser": "^6.17.0",
    "@vuepress/bundler-vite": "^2.0.0-rc.19",
    "@vuepress/theme-default": "^2.0.0-rc.78",
    "chai": "4.4.1",
    "mocha": "^10.4.0",
    "nock": "14.0.0-beta.7",
    "nyc": "^15.1.0",
    "prettier": "^3.3.2",
    "sass-embedded": "^1.85.0",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5",
    "vue": "^3.5.13",
    "vuepress": "^2.0.0-rc.19"
  }
}
